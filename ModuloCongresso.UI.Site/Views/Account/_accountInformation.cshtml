@model ModuloCongresso.Application.ViewModels.ClienteEnderecoViewModel

<div class="col-lg-8">
    <div class="form-group">
        <label>Email *</label>
        @Html.TextBoxFor(m => m.Email, new { @class = "form-control required email" })
    </div>
    <div class="form-group">
        <label>Password *</label>
        @Html.PasswordFor(m => m.Password, new { @class = "form-control required" })
    </div>
    <div class="form-group">
        <label>Confirm Password *</label>
        @Html.PasswordFor(m => m.ConfirmPassword, new { @class = "form-control required" })
    </div>
</div>