@model ModuloCongresso.Application.ViewModels.ClienteEnderecoViewModel

<div class="col-lg-6">
    <div class="form-group">
        <label>Name *</label>
        @Html.EditorFor(model => model.Nome, new { htmlAttributes = new { @class = "form-control required" } })
    </div>
    <div class="form-group">
        <label>Sobrenome *</label>
        @Html.EditorFor(model => model.SobreNome, new { htmlAttributes = new { @class = "form-control required" } })
    </div>
    <div class="form-group">
        <label>CPF *</label>
        @Html.EditorFor(model => model.CPF, new { htmlAttributes = new { @class = "form-control required" } })
    </div>
    <div class="form-group">
        <label>RG *</label>
        @Html.EditorFor(model => model.RG, new { htmlAttributes = new { @class = "form-control required" } })
    </div>
</div>
<div class="col-lg-6">
    <div class="form-group">
        <label>Telefone *</label>
        @Html.EditorFor(model => model.telefone, new { htmlAttributes = new { @class = "form-control required" } })
    </div>
    <div class="form-group">
        <label>Data de Nascimento *</label>
        @Html.EditorFor(model => model.DataNascimento, new { htmlAttributes = new { @class = "form-control required" } })
    </div>
    <div class="form-group">
        <label>Profissão *</label>
        @Html.DropDownListFor(model => model.ProfissaoId, Model.Profissoes, "-- Select Profissão", new { @class = "form-control required" })
    </div>
    <div class="form-group">
        <label>Nacionalidade *</label>
        @Html.DropDownListFor(model => model.PaisResidenciaId, Model.Pais, "-- Select Pais Residencia", new { @class = "form-control required" })
    </div>
</div>